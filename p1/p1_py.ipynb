   "source": [
    "We just need to find all integers $< 1000$ which are divisible by 3 or 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "def sum_multiples(n):\n",
    "    f = lambda x: x if (x % 3 == 0 or x % 5 == 0) else 0\n",
    "    return sum(np.array(list(map(f, np.arange(1, n)))))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
