    "function R(n)\n",
    "   R = BigFloat(1)\n",
    "   for i in 1:n\n",
    "       R = (1 / i^2) * ((i^2 - 1)*R + 2* i - 1)\n",
    "   end\n",
    "   return R\n",
    "end"
